[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="keep"

[autoload]

Log="*res://scripts/logger.gd"
GameBus="*res://scripts/game_bus.gd"
NetworkManager="*res://addons/netcode/network_manager.gd"
DemoLauncher="*res://addons/netcode/demo_launcher.gd"
GameMenu="*res://addons/elegant_menu/game_menu.tscn"

[editor_plugins]

enabled=PackedStringArray("res://addons/elegant_menu/plugin.cfg", "res://addons/gut/plugin.cfg", "res://addons/netcode/plugin.cfg")

